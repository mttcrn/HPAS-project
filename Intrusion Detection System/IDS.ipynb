      "source": [
        "def compute_entropy_fast(hex_str):\n",
        "    if not isinstance(hex_str, str) or len(hex_str) != 16:\n",
        "        return 0.0\n",
        "    byte_array = np.frombuffer(bytes.fromhex(hex_str), dtype=np.uint8)\n",
        "    counts = np.bincount(byte_array, minlength=256)\n",
        "    probs = counts[counts > 0] / len(byte_array)\n",
        "    return -np.sum(probs * np.log2(probs))"
      ]
    },
    {

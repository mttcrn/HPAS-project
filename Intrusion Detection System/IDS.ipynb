        "def compute_hamming_distance(b1, b2):\n",
        "    \"\"\"Compute Hamming distance between two byte strings.\"\"\"\n",
        "    return sum(bin(x ^ y).count('1') for x, y in zip(b1, b2))"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 10,
      "id": "LPdm3KwyvU1f",
      "metadata": {
        "id": "LPdm3KwyvU1f"
      },
      "outputs": [],
      "source": [
        "def hex_to_bytes(hex_str):\n",
        "    \"\"\"Convert 16-char hex payload to bytes object.\"\"\"\n",
        "    if isinstance(hex_str, str) and len(hex_str) == 16:\n",
        "        return bytes.fromhex(hex_str)\n",
        "    return b'\\x00' * 8  # fallback for malformed data"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 11,
      "id": "Z6M41wt1vYWg",
      "metadata": {
        "id": "Z6M41wt1vYWg"
      },
      "outputs": [],
      "source": [
        "def add_hamming_feature(df):\n",
        "    df = df.copy()\n",
        "    df['data_bytes'] = df['data_field'].apply(hex_to_bytes)\n",
        "    df['hamming_dist_prev'] = 0\n",
        "\n",
        "    for msg_id, group in df.groupby('arbitration_id'):\n",
        "        group_sorted = group.sort_values('timestamp')\n",
        "        indices = group_sorted.index.tolist()\n",
        "        payloads = list(group_sorted['data_bytes'])\n",
        "\n",
        "        for i in range(1, len(payloads)):\n",
        "            dist = compute_hamming_distance(payloads[i-1], payloads[i])\n",
        "            df.at[indices[i], 'hamming_dist_prev'] = dist\n",
        "\n",
        "    return df.drop(columns=['data_bytes'])"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 12,
      "id": "ehM90GQz0nnl",
      "metadata": {
        "id": "ehM90GQz0nnl"
      },
      "outputs": [],
      "source": [
        "def extract_data_field_features(df):\n",
        "    \"\"\"Extract byte-level and statistical features from the data_field.\"\"\"\n",
        "    df = df.copy()\n",
        "\n",
        "    # Convert data_field to list of bytes\n",
        "    df['data_bytes'] = df['data_field'].apply(hex_to_bytes)\n",
        "\n",
        "    # Extract each byte into its own column\n",
        "    byte_columns = pd.DataFrame(\n",
        "        df['data_bytes'].apply(lambda b: list(b)).tolist(),\n",
        "        columns=[f'byte_{i}' for i in range(8)]\n",
        "    ).astype(np.float32)\n",
        "\n",
        "    # Optional: also add total integer value of payload\n",
        "    df['payload_value'] = df['data_field'].apply(lambda x: int(x, 16) if isinstance(x, str) and len(x) == 16 else 0)\n",
        "    byte_columns['payload_value'] = df['payload_value']\n",
        "\n",
        "    return byte_columns"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 13,
      "id": "nRGe4woC60yK",
      "metadata": {
        "id": "nRGe4woC60yK"

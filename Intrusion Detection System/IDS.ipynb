        "!pip install swifter\n",
        "import swifter # used to parallelize parallelizes .apply() in computing the entropy\n",
        "\n",
        "def add_entropy_delta(df):\n",
        "    df = df.copy()\n",
        "\n",
        "    # Compute entropy per packet\n",
        "    df['entropy'] = df['data_field'].swifter.apply(compute_entropy_fast)\n",
        "\n",
        "    # Sort by arbitration_id and timestamp\n",
        "    df = df.sort_values(['arbitration_id', 'timestamp'])\n",
        "\n",
        "    # Compute entropy delta as difference with previous entropy in same arbitration_id group\n",
        "    df['entropy_delta'] = df.groupby('arbitration_id')['entropy'].diff().fillna(0)\n",
        "\n",
        "    return df['entropy_delta']"
      ]
    },
